// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Participant {
  id  Int @default(autoincrement()) @id
  name  String
  is_coming Boolean?
  has_submit  Boolean
  phone_number  String? @unique
  email String?  @unique
}